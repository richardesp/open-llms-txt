# {{ h1 or title }}

{% for paragraph in paragraphs %}
{{ paragraph }}

{% endfor %}

{% if headings %}
## Sections
{% for heading in headings %}
- {{ heading }}
{% endfor %}
{% endif %}

{% if links %}
## Links
{% for link in links %}
- [{{ link.text }}](
  {%- if link.href.startswith("http") -%} 
    {{ link.href }}
  {%- else -%}
    {{ metadata.root_url.rstrip('/') }}/
    {%- if link.href.startswith("./") -%}{{ link.href[2:] }}
    {%- elif link.href.startswith("/") -%}{{ link.href[1:] }}
    {%- else -%}{{ link.href }}{% endif %}
  {%- endif -%}
)
{% endfor %}
{% endif %}